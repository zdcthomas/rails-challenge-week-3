<%= form_for [@student, @address] do |f| %> %>
  <%= f.label :street_address %>
  <%= f.text_field :street_address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zip_code %>
  <%= f.number_field :zip_code %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit%>
<% end %>